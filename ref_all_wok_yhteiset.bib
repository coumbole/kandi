@InProceedings{ISI:000393314500081,
  Title                    = {{The Research of Testing Process and Data Tracing Based on Hybrid Model}},
  Author                   = {Liu, Xiaochen and Yan, Haihua and Sun, Yang},
  Booktitle                = {{PROCEEDINGS OF 2015 4TH INTERNATIONAL CONFERENCE ON COMPUTER SCIENCE AND
 NETWORK TECHNOLOGY (ICCSNT 2015)}},
  Year                     = {{2015}},
