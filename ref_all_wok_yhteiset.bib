  Eissn                    = {{1875-8835}},
  ISSN                     = {{1069-2509}},
  Orcid-numbers            = {{Hilaire, Vincent/0000-0003-2725-7140}},
  Researcherid-numbers     = {{Hilaire, Vincent/C-7022-2011}},
  Unique-id                = {{ISI:000355956700005}}
}

