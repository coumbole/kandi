  ISBN                     = {{978-3-319-38980-6; 978-3-319-38979-0}},
  ISSN                     = {{1865-0929}},
  Orcid-numbers            = {{Clarke, Paul/0000-0002-4487-627X}},
  Unique-id                = {{ISI:000382651100025}}
}

