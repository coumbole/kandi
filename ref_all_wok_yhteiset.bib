@InProceedings{ISI:000349866100246,
  Title                    = {{Moving Agile based projects on Cloud}},
  Author                   = {Manuja, Manoj and Manisha},
  Booktitle                = {{SOUVENIR OF THE 2014 IEEE INTERNATIONAL ADVANCE COMPUTING CONFERENCE
 (IACC)}},
  Year                     = {{2014}},
  Editor                   = {{Batra, U and Sujata and Arpita}},
  Note                     = {{4th IEEE International Advance Computing Conference (IACC), ITM Univ,
 Gurgaon, INDIA, FEB 21-22, 2014}},
  Organization             = {{IEEE; ITM Univ, Dept Comp Sci Engn \& Informat Technol; IEEE Comp Soc;
 IEEE Student Branch}},
  Pages                    = {{1392-1397}},
  Series                   = {{IEEE International Advance Computing Conference}},

  Abstract                 = {{Cost, time and quality (CTQ) have been the primary factors behind
 continuous advancements in the field of Software development
 methodologies. Waterfall model has been with the software industry for
 over 30 years offering a sequential and linear development approach.
 However, the impending issues with regards to CTQ have led to rise of
 agile development methodologies in last couple of decades offering
 incremental \& iterative development, customer collaboration and reduced
 time to market. The new agile paradigm, on the other side, is also
 bringing its own challenges and risks primarily in terms of runtime
 infrastructure cost, network accessibility and platform availability
 steering towards CTQ issues in a disguised manner. With Cloud Computing
 gaining popularity over the last few years offering almost zero CAPEX
 (Capital Expenditure), reduced OPEX (Operational Expenditure), high
 scalability \& availability, it has been considered worthwhile to
 explore Agile based software development on Cloud. In this article,
 migration of Agile based project to Cloud has been discussed along with
 its benefits and challenges at both the stages. While Cloud adoption is
 a catalyst for Agile based development, there are a few open issues to
 be addressed.}},
  ISBN                     = {{978-1-4799-2571-1}},
  ISSN                     = {{2164-8263}},
  Unique-id                = {{ISI:000349866100246}}
}

