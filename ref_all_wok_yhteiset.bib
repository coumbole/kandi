@InProceedings{ISI:000363326300058,
  Title                    = {{Tualatin: Towards Network Security Service Provision in Cloud
 Datacenters}},
  Author                   = {Wang, Xiang and Liu, Zhi and Li, Jun and Yang, Baohua and Qi, Yaxuan},
  Booktitle                = {{2014 23RD INTERNATIONAL CONFERENCE ON COMPUTER COMMUNICATION AND
 NETWORKS (ICCCN)}},
  Year                     = {{2014}},
  Note                     = {{23rd International Conference on Computer Communication and Networks
 (ICCCN), Shanghai, PEOPLES R CHINA, AUG 04-07, 2014}},
  Organization             = {{IEEE; Tongji Univ; NSF}},

  Abstract                 = {{Multi-tenant infrastructures deployed in cloud datacenters need network
 security protection. However, the rigid control mechanism of current
 security middleboxes induces inflexible orchestration, limiting the
 agile and on-demand security provision in virtualized datacenters. This
 paper presents Tualatin, a consolidated framework of delivering security
 services in multi-tenant datacenters. It meets security requirements of
 different scenarios by hardware and software co-design. Leveraging
 Software-Defined Networking (SDN) and OpenFlow techniques, Tualatin
 provides fine-grained security protection in dynamically changing
 network topologies, where both switches and security middleboxes are
 programmatically controlled by logically centralized controllers. With
 service-level APIs exposed, Tualatin could be easily integrated with
 other Cloud Management System (CMS). A proof-of-concept system has been
 deployed in a Tier-IV datacenter, providing customizable network
 security services for tenant Virtual Private Cloud (VPC) infrastructure.}},
  Book-group-author        = {{IEEE}},
  ISBN                     = {{978-1-4799-3572-7}},
  Unique-id                = {{ISI:000363326300058}}
}

