@InProceedings{ISI:000325717400026,
  Title                    = {{Service Agile Development Using XP}},
  Author                   = {Carvalho, Felipe and Azevedo, Leonardo Guerreiro},
  Booktitle                = {{2013 IEEE SEVENTH INTERNATIONAL SYMPOSIUM ON SERVICE-ORIENTED SYSTEM ENGINEERING (SOSE 2013)}},
  Year                     = {{2013}},
